<p id="notice"><%= notice %></p>

<h1>Entries</h1>

<% @entries.reverse_each do |entry| %>
  <div class="well" style="background-color: #213946">
    <h3><%= entry.title %></h3>
    <p><%= entry.body[0...500] %></p>
    <div style="text-align: right"><%= link_to "Read More", entry_path(entry), :class => 'btn btn-default' %></div>
  </div>

<br>
    <% end %>


